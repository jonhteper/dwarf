{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "camelcase": ["error", {"properties": "always"}],
    "vue/camelcase": ["error", {"properties": "always"}],
    "vue/valid-template-root": "off"
  }
}
